{
  "name": "mimloader",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "lint": "bunx @biomejs/biome check src",
    "lint:fix": "bun lint --apply",
    "bundle": "esbuild --bundle src/index.ts --outfile=out/index.js --platform=node --format=cjs --target=node18 --packages=external",
    "build": "bun run lint && bun test && bun run bundle && pkg ./out/index.js --out-path ./bin --targets node18-linux-x64,node18-macos-x64,node18-win-x64"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "esbuild": "^0.20.2",
    "pkg": "^5.8.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "esbuild"
  ],
  "dependencies": {
    "compressing": "^1.10.0",
    "express": "^4.18.3",
    "fs-extra": "^11.2.0",
    "ws": "^8.16.0"
  }
}